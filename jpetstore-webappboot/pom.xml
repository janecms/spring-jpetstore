    <modelVersion>4.0.0</modelVersion>
    <groupId>com.hellojd.jpetstore</groupId>
    <artifactId>jpetstore-webappboot</artifactId>
    <version>1.0.0</version>
    <packaging>war</packaging>
    <name>jpetstore-webappboot</name>
    <url>http://maven.apache.org</url>
    <dependencies>
        <dependency>
            <groupId>com.hellojd.jpetstore</groupId>
